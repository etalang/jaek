use io
use conv
foo(){
    println(unparseInt((4 - 5)))
    println(unparseInt(2 * 3 * (4 - 5)))
    println(unparseInt(2 * 3 * (4 - 5) / 6))
    println(unparseInt(2 * 3 * (4 - 5) / 6 *>> 7))
    println(unparseInt(2 * 3 * (4 - 5) / 6 *>> 7 % 8))
    println(unparseInt(1 + 2 * 3 * (4 - 5) / 6 *>> 7 % 8))
}

main(args : int[][]){
    foo()
}