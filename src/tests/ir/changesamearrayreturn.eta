populateArray(aa : int[][]) : int[][]{
    i : int = 0
    while (i < length(aa)) {
        j : int = 0
        while (j < length(aa[i])) {
            aa[i][j] = i * j
            j = j + 1
        }
        i = i + 1
    }
    return aa
}

printArray(aa : int[]) {
    print("{")
    i : int = 0
    while (i < length(aa)) {
        print(unparseInt(aa[i]))
        if (i < length(aa) - 1) print(" ")
        i = i + 1
    }
    print("}")
    println("")
}

sort(aa: int[]) {
  i:int = 0
  n:int = length(aa)
  while (i < n) {
      j:int = i
      while (j > 0) {
        if (aa[j-1] > aa[j]) {
            swap:int = aa[j]
            aa[j] = aa[j-1]
            aa[j-1] = swap
        }
        j = j-1
      }
      i = i+1
  }
}

