use io
use conv

f(arr:int[]) {
    arr[0] = 17
}

main(args:int[][])  {
    d : int[] = {13, 6, 4, 'a', 's', 9, 5, 3, 2}
    f(d)
    printArray(d)
}

printArray(aa : int[]) {
    print("{")
    i : int = 0
    while (i < length(aa)) {
        print(unparseInt(aa[i]))
        if (i < length(aa) - 1) print(" ")
        i = i + 1
    }
    print("}")
    println("")
}