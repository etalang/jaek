populateArray(aa : int[][]) {
    i : int = 0
    while (i < length(aa)) {
        j : int = 0
        while (j < length(aa[i])) {
            aa[i][j] = i * j
            j = j + 1
        }
        i = i + 1
    }
}

printArray(aa : int[]) {
    i : int = 0
    while (i < length(aa)) {
        print(unparseInt(aa[i]))
        print(" ")
        i = i + 1
    }
    println("")
}

sort(aa: int[]) {
  i:int = 0
  n:int = length(a)
  while (i < n) {
      j:int = i
      while (j > 0) {
        if (a[j-1] > a[j]) {
            swap:int = a[j]
            a[j] = a[j-1]
            a[j-1] = swap
        }
        j = j-1
      }
      i = i+1
  }
}

