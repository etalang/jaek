etac ("Test --typecheck") "-libpath $(testpath) --typecheck" {
    badeti.eti (Semantic, "Mismatch in type of function f among interfaces/programs");
    badetiparse.eti (Syntax, "use");
    badetiparseuse.eta (Syntax, "use");
