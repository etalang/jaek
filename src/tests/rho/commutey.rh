use io
use conv

a : int = 3

main(args:int[][])  {
    x : int = 5
    x = a + f(x)

    print(unparseInt(x))
}

f(x : int) : int {
    a = a + 1
    return x + 1
}

populateArray(aa : int[][]) : int[][]{
    i : int = 0
    while (i < length(aa)) {
        j : int = 0
        while (j < length(aa[i])) {
            aa[i][j] = i * j
            j = j + 1
        }
        i = i + 1
    }
    return aa
}

printArray(aa : int[]) {
    print("{")
    i : int = 0
    while (i < length(aa)) {
        print(unparseInt(aa[i]))
        if (i < length(aa) - 1) print(" ")
        i = i + 1
    }
    print("}")
    println("")
}

