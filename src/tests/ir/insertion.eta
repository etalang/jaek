use conv
use io

sort(a: int[]) {
  i:int = 0
  n:int = length(a)
  while (i < n) {
      j:int = i
      while (j > 0) {
        if (a[j-1] > a[j]) {
            swap:int = a[j]
            a[j] = a[j-1]
            a[j-1] = swap
        }
        j = j-1
      }
      i = i+1
  }
}

main(argv:int[][]) {
    a:int[] = {1, 23, 69, 42, 4913, 17}
    sort(a)
    i:int = 0
    while (i < length(a)) {
        println(unparseInt(a[i]))
        i = i + 1
    }
}